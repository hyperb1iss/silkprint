#set document(
  title: "Basic Test Document",
  author: ("SilkPrint Test Suite",),
)

#set page(
  paper: "a4",
  margin: (top: 25mm, bottom: 30mm, left: 20mm, right: 20mm),
  fill: rgb("#faf8ff"),
  numbering: "1",
  number-align: center + bottom,
)

#set raw(theme: "/__silkprint_theme.tmTheme")

#set text(
  font: ("JetBrains Mono", "JetBrains Mono", "Fira Code", "SF Mono", "Cascadia Code"),
  size: 10.5pt,
  fill: rgb("#2b2540"),
  lang: "en",
  hyphenate: true,
  ligatures: true,
)

#set par(
  justify: true,
  leading: 0.50em,
  spacing: 0.85em,
)

#show heading.where(level: 1): it => {
  v(36pt)
  block(below: 12pt)[
    #set text(font: "Inter", size: 24pt, weight: 700, fill: rgb("#1565c0"), tracking: -0.015em)
    #set par(leading: 0.10em)
    #it.body
  ]
  line(length: 100%, stroke: 0.5pt + rgb("#d8d0ea"))
}

#show heading.where(level: 2): it => {
  v(28pt)
  block(below: 8pt)[
    #set text(font: "Inter", size: 19pt, weight: 600, fill: rgb("#1565c0"), tracking: -0.015em)
    #set par(leading: 0.15em)
    #it.body
  ]
  line(length: 100%, stroke: 0.5pt + rgb("#d8d0ea"))
}

#show heading.where(level: 3): it => {
  v(22pt)
  block(below: 6pt)[
    #set text(font: "Inter", size: 16pt, weight: 600, fill: rgb("#1565c0"), tracking: -0.015em)
    #set par(leading: 0.20em)
    #it.body
  ]
}

#show heading.where(level: 4): it => {
  v(18pt)
  block(below: 4pt)[
    #set text(font: "Inter", size: 13pt, weight: 500, fill: rgb("#1565c0"), tracking: -0.015em)
    #set par(leading: 0.20em)
    #it.body
  ]
}

#show heading.where(level: 5): it => {
  v(18pt)
  block(below: 4pt)[
    #set text(font: "Inter", size: 11.5pt, weight: 500, fill: rgb("#1565c0"), tracking: -0.015em)
    #set par(leading: 0.25em)
    #it.body
  ]
}

#show heading.where(level: 6): it => {
  v(18pt)
  block(below: 4pt)[
    #set text(font: "Inter", size: 10.5pt, weight: 600, fill: rgb("#1565c0"), tracking: 0.05em)
    #set par(leading: 0.25em)
    #upper(it.body)
  ]
}

#show raw.where(block: true): it => {
  block(
    fill: rgb("#f1ecff"),
    stroke: 0.5pt + rgb("#d8d0ea"),
    radius: 6pt,
    inset: (x: 14pt, y: 12pt),
    width: 100%,
    breakable: true,
  )[
    #set text(font: "JetBrains Mono", size: 9.5pt, ligatures: false)
    #set par(justify: false, leading: 0.45em)
    #it
  ]
}

#show raw.where(block: false): it => {
  box(
    fill: rgb("#f1ecff"),
    stroke: 0.5pt + rgb("#d8d0ea"),
    radius: 3pt,
    inset: (x: 3pt, y: 1.5pt),
  )[
    #set text(font: "JetBrains Mono", size: 9.5pt, ligatures: false)
    #it
  ]
}

#show link: it => {
  set text(fill: rgb("#1565c0"), ligatures: false)
  underline(it)
}

#show quote.where(block: true): it => {
  block(
    stroke: (left: 2.5pt + rgb("#a82890")),
    inset: (left: 14pt, y: 8pt, right: 8pt),
    width: 100%,
  )[
    #set text(fill: rgb("#544c6e"))
    #emph(it.body)
  ]
}

#set table(
  stroke: none,
  inset: (x: 10pt, y: 6pt),
)
#show table.cell.where(y: 0): set text(font: "Inter", weight: 600, fill: rgb("#1565c0"))
